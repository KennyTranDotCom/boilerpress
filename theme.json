{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "border": {
            "color": false,
            "style": false,
            "width": false
        },
        "color": {
            "customDuotone": false,
            "customGradient": false,
            "defaultPalette": false,
            "defaultGradients": false,
            "duotone": [],
            "gradients": [],
            "palette": [
                {
                    "slug": "primary",
                    "color": "#3d44ff",
                    "name": "Primary"
                },
                {
                    "slug": "black",
                    "color": "#080808",
                    "name": "Black"
                },
                {
                    "slug": "grey-dark",
                    "color": "#444444",
                    "name": "Grey - Dark"
                },
                {
                    "slug": "grey",
                    "color": "#888888",
                    "name": "Grey"
                },
                {
                    "slug": "grey-light",
                    "color": "#b6b4b4",
                    "name": "Grey - Light"
                },
                {
                    "slug": "white",
                    "color": "#fdfdfd",
                    "name": "White"
                }
            ]
        },
        "layout": {
            "contentSize": "var(--wp--custom--layout--content)",
            "wideSize": "var(--wp--custom--layout--wide)"
        },
        "spacing": {
            "spacingScale": {
                "steps": 0
            },
            "spacingSizes": [
                {
                    "name": "4X Small",
                    "size": "clamp(0.33rem, calc(-0.03vw + 0.33rem), 0.31rem)",
                    "slug": "4-x-small"
                },
                {
                    "name": "3X Small",
                    "size": "clamp(0.41rem, calc(0.04vw + 0.4rem), 0.44rem)",
                    "slug": "3-x-small"
                },
                {
                    "name": "2X Small",
                    "size": "clamp(0.51rem, calc(0.15vw + 0.48rem), 0.62rem)",
                    "slug": "2-x-small"
                },
                {
                    "name": "X Small",
                    "size": "clamp(0.64rem, calc(0.34vw + 0.57rem), 0.88rem)",
                    "slug": "x-small"
                },
                {
                    "name": "Small",
                    "size": "clamp(0.8rem, calc(0.63vw + 0.68rem), 1.24rem)",
                    "slug": "small"
                },
                {
                    "name": "Medium",
                    "size": "clamp(1rem, calc(1.07vw + 0.79rem), 1.75rem)",
                    "slug": "medium"
                },
                {
                    "name": "Large",
                    "size": "clamp(1.25rem, calc(1.75vw + 0.9rem), 2.47rem)",
                    "slug": "large"
                },
                {
                    "name": "X Large",
                    "size": "clamp(1.56rem, calc(2.77vw + 1.01rem), 3.5rem)",
                    "slug": "x-large"
                },
                {
                    "name": "2X Large",
                    "size": "clamp(1.95rem, calc(4.28vw + 1.1rem), 4.95rem)",
                    "slug": "2-x-large"
                },
                {
                    "name": "3X Large",
                    "size": "clamp(2.44rem, calc(6.51vw + 1.14rem), 7rem)",
                    "slug": "3-x-large"
                },
                {
                    "name": "4X Large",
                    "size": "clamp(3.05rem, calc(9.77vw + 1.1rem), 9.89rem)",
                    "slug": "4-x-large"
                }
            ],
            "units": ["%", "px", "em", "rem", "vh", "vw"]
        },
        "typography": {
            "dropCap": false,
            "customFontSize": false,
            "fluid": true,
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "PP Mori",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": ["file:./build/fonts/PPMori-Regular.woff2"]
                        }
                    ],
                    "fontFamily": "\"PP Mori\", sans-serif",
                    "name": "Mori",
                    "slug": "mori"
                },
                {
                    "fontFace": [
                        {
                            "fontFamily": "Inter",
                            "fontStyle": "normal",
                            "fontWeight": "400 600",
                            "src": [
                                "file:./build/fonts/inter/Inter-VariableFont_slnt,wght.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "\"Inter\", serif",
                    "name": "Inter",
                    "slug": "inter"
                }
            ],
            "fontSizes": [
                {
                    "name": "5X Large",
                    "size": "clamp(2.03rem, calc(6.12vw + 0.8rem), 6.31rem)",
                    "slug": "5-x-large"
                },
                {
                    "name": "4X Large",
                    "size": "clamp(1.8rem, calc(4.19vw + 0.96rem), 4.74rem)",
                    "slug": "4-x-large"
                },
                {
                    "name": "3X Large",
                    "size": "clamp(1.6rem, calc(2.79vw + 1.04rem), 3.55rem)",
                    "slug": "3-x-large"
                },
                {
                    "name": "2X Large",
                    "size": "clamp(1.42rem, calc(1.77vw + 1.07rem), 2.66rem)",
                    "slug": "2-x-large"
                },
                {
                    "name": "X Large",
                    "size": "clamp(1.27rem, calc(1.05vw + 1.06rem), 2rem)",
                    "slug": "x-large"
                },
                {
                    "name": "Large",
                    "size": "clamp(1.13rem, calc(0.37vw + 1.05rem), 1.5rem)",
                    "slug": "large"
                },
                {
                    "name": "Medium",
                    "size": "clamp(1rem, calc(0.18vw + 0.96rem), 1.13rem)",
                    "slug": "medium"
                },
                {
                    "name": "Small",
                    "size": "clamp(0.89rem, calc(-0.06vw + 0.9rem), 0.84rem)",
                    "slug": "small"
                },
                {
                    "name": "X Small",
                    "size": "clamp(0.79rem, calc(-0.22vw + 0.83rem), 0.63rem)",
                    "slug": "x-small"
                }
            ]
        },
        "blocks": {
            "core/heading": {
                "color": {
                    "background": false
                }
            }
        },
        "useRootPaddingAwareAlignments": true,
        "custom": {
            "border-radius": {
                "x-small": "4px",
                "small": "8px",
                "medium": "12px",
                "large": "50%"
            },
            "border-width": {
                "x-small": "1px",
                "small": "2px",
                "medium": "3px",
                "large": "4px"
            },
            "box-shadow": {
                "x-small": "0 1px 2px var(--wp--preset--color--shadow-on-background)",
                "small": "0 1.5px 3px var(--wp--preset--color--shadow-on-background)",
                "medium": "0 2px 6px var(--wp--preset--color--shadow-on-background)",
                "large": "0 3px 12px var(--wp--preset--color--shadow-on-background)",
                "x-large": "0 6px 48px var(--wp--preset--color--shadow-on-background)"
            },
            "color": {
                "primary": "var(--wp--preset--color--primary)",
                "on-primary": "var(--wp--preset--color--white)",
                "secondary": "var(--wp--preset--color--secondary)",
                "on-secondary": "var(--wp--preset--color--white)",
                "background": "var(--wp--preset--color--black)",
                "on-background": "var(--wp--preset--color--grey-light)",
                "light": "var(--wp--preset--color--white)",
                "on-light": "var(--wp--preset--color--black)",
                "dark": "var(--wp--preset--color--black)",
                "on-dark": "var(--wp--preset--color--white)"
            },
            "layout": {
                "content": "1680px",
                "padding": "calc(clamp(1.25rem, 0.605rem + 3.226vw, 3.75rem))",
                "padding-negative": "calc(var(--wp--custom--layout--padding) * -1)",
                "block-gap": "var(--wp--preset--spacing--medium)"
            },
            "letter-spacing": {
                "none": "normal",
                "tight": "-.01em",
                "loose": ".05em",
                "looser": ".1em"
            },
            "line-height": {
                "none": 1,
                "3-x-small": 1.1,
                "2-x-small": 1.2,
                "x-small": 1.3,
                "small": 1.4,
                "medium": 1.5,
                "large": 1.6
            }
        }
    },
    "styles": {
        "blocks": {},
        "color": {
            "background": "var(--wp--custom--color--background)",
            "text": "var(--wp--custom--color--on-background)"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--custom--color--on-background)"
                }
            },
            "button": {
                "color": {
                    "background": "var(--wp--custom--color--primary)",
                    "text": "var(--wp--custom--color--on-primary)"
                },
                "border": {
                    "radius": "0px",
                    "width": "0px"
                },
                "spacing": {
                    "padding": {
                        "bottom": "0.8em",
                        "left": "1.6em",
                        "right": "1.6em",
                        "top": "0.8em"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "var(--wp--custom--line-height--none)"
                }
            },
            "heading": {
                "color": {
                    "text": "var(--wp--preset--color--white)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--mori)",
                    "fontWeight": "400",
                    "lineHeight": "var(--wp--custom--line-height--3-x-small)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--4-x-large)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3-x-large)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--2-x-large)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            }
        },
        "spacing": {
            "blockGap": "var(--wp--custom--layout--block-gap)",
            "padding": {
                "right": "var(--wp--custom--layout--padding)",
                "left": "var(--wp--custom--layout--padding)"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--inter)",
            "lineHeight": "var(--wp--custom--line-height--medium)",
            "fontSize": "var(--wp--preset--font-size--medium)"
        }
    }
}
