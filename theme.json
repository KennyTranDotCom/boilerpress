{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "border": {
            "color": false,
            "radius": false,
            "style": false,
            "width": false
        },
        "color": {
            "custom": false,
            "customDuotone": false,
            "customGradient": false,
            "defaultPalette": false,
            "defaultGradients": false,
            "duotone": [],
            "gradients": [],
            "link": false,
            "palette": [
                {
                    "slug": "primary",
                    "color": "#3d44ff",
                    "name": "Primary"
                },
                {
                    "slug": "background",
                    "color": "#080808",
                    "name": "Background"
                },
                {
                    "slug": "on-background",
                    "color": "#fdfdfd",
                    "name": "On Background"
                },
                {
                    "slug": "shadow-on-background",
                    "color": "#080808",
                    "name": "Shadow On Background"
                },
                {
                    "slug": "surface",
                    "color": "#fdfdfd",
                    "name": "Surface"
                },
                {
                    "slug": "on-surface",
                    "color": "#080808",
                    "name": "On Surface"
                }
            ]
        },
        "custom": {
            "border-radius": {
                "x-small": "3px",
                "small": "8px",
                "medium": "12px",
                "large": "50%"
            },
            "border-width": {
                "x-small": "1px",
                "small": "2px",
                "medium": "3px",
                "large": "4px"
            },
            "box-shadow": {
                "x-small": "0 1px 2px var(--wp--preset--color--shadow-on-background)",
                "small": "0 1.5px 3px var(--wp--preset--color--shadow-on-background)",
                "medium": "0 2px 6px var(--wp--preset--color--shadow-on-background)",
                "large": "0 3px 12px var(--wp--preset--color--shadow-on-background)",
                "x-large": "0 6px 48px var(--wp--preset--color--shadow-on-background)"
            },
            "color": {
                "base": "#fdfdfd",
                "base-d-1": "#d5d5d5",
                "base-d-2": "#aeaeae",
                "base-d-3": "#888888",
                "base-d-4": "#646464",
                "base-d-5": "#434343",
                "base-d-6": "#242424",
                "link": "var(--wp--preset--color--base)"
            },
            "layout": {
                "content": "1440px",
                "wide": "1920px",
                "page": "var(--wp--custom--layout--content)",
                "padding": "16px",
                "block-gap": "16px",
                "block-gap-large": "40px"
            },
            "letter-spacing": {
                "none": "normal",
                "tight": "-.01em",
                "loose": ".05em",
                "looser": ".1em"
            },
            "line-height": {
                "x-small": 1.1,
                "small": 1.2,
                "medium": 1.4,
                "normal": 1.6
            }
        },
        "layout": {
            "contentSize": "var(--wp--custom--layout--content)",
            "wideSize": "var(--wp--custom--layout--wide)"
        },
        "spacing": {
            "spacingScale": {
                "steps": 0
            },
            "spacingSizes": [
                {
                    "name": "4X Small",
                    "size": "clamp(0.52rem, calc(-0.03vw + 0.53rem), 0.49rem)",
                    "slug": "4-x-small"
                },
                {
                    "name": "3X Small",
                    "size": "clamp(0.66rem, calc(0.04vw + 0.64rem), 0.7rem)",
                    "slug": "3-x-small"
                },
                {
                    "name": "2X Small",
                    "size": "clamp(0.82rem, calc(0.16vw + 0.77rem), 0.99rem)",
                    "slug": "2-x-small"
                },
                {
                    "name": "X Small",
                    "size": "clamp(1.02rem, calc(0.35vw + 0.91rem), 1.4rem)",
                    "slug": "x-small"
                },
                {
                    "name": "Small",
                    "size": "clamp(1.28rem, calc(0.65vw + 1.07rem), 1.98rem)",
                    "slug": "small"
                },
                {
                    "name": "Medium",
                    "size": "clamp(1.6rem, calc(1.11vw + 1.24rem), 2.8rem)",
                    "slug": "medium"
                },
                {
                    "name": "Large",
                    "size": "clamp(2rem, calc(1.81vw + 1.42rem), 3.96rem)",
                    "slug": "large"
                },
                {
                    "name": "X Large",
                    "size": "clamp(2.5rem, calc(2.87vw + 1.58rem), 5.6rem)",
                    "slug": "x-large"
                },
                {
                    "name": "2X Large",
                    "size": "clamp(3.13rem, calc(4.44vw + 1.71rem), 7.92rem)",
                    "slug": "2-x-large"
                },
                {
                    "name": "3X Large",
                    "size": "clamp(3.91rem, calc(6.75vw + 1.75rem), 11.19rem)",
                    "slug": "3-x-large"
                },
                {
                    "name": "4X Large",
                    "size": "clamp(4.88rem, calc(10.13vw + 1.64rem), 15.83rem)",
                    "slug": "4-x-large"
                }
            ],
            "units": ["%", "px", "em", "rem", "vh", "vw"]
        },
        "typography": {
            "dropCap": false,
            "lineHeight": false,
            "customFontSize": false,
            "letterSpacing": false,
            "fluid": true,
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "PP Mori",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": ["file:./build/fonts/PPMori-Regular.woff2"]
                        }
                    ],
                    "fontFamily": "\"PP Mori\", sans-serif",
                    "name": "Mori",
                    "slug": "mori"
                },
                {
                    "fontFace": [
                        {
                            "fontFamily": "Inter",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": ["file:./build/fonts/Inter-Regular.woff2"]
                        }
                    ],
                    "fontFamily": "\"Inter\", serif",
                    "name": "Inter",
                    "slug": "inter"
                }
            ],
            "fontSizes": [
                {
                    "name": "5X Large",
                    "size": "clamp(2.03rem, calc(4.28vw + 1.17rem), 6.31rem)",
                    "slug": "5-x-large"
                },
                {
                    "name": "4X Large",
                    "size": "clamp(1.8rem, calc(2.93vw + 1.22rem), 4.74rem)",
                    "slug": "4-x-large"
                },
                {
                    "name": "3X Large",
                    "size": "clamp(1.6rem, calc(1.95vw + 1.21rem), 3.55rem)",
                    "slug": "3-x-large"
                },
                {
                    "name": "2X Large",
                    "size": "clamp(1.42rem, calc(1.24vw + 1.18rem), 2.66rem)",
                    "slug": "2-x-large"
                },
                {
                    "name": "X Large",
                    "size": "clamp(1.27rem, calc(0.73vw + 1.12rem), 2rem)",
                    "slug": "x-large"
                },
                {
                    "name": "Large",
                    "size": "clamp(1.13rem, calc(0.37vw + 1.05rem), 1.5rem)",
                    "slug": "large"
                },
                {
                    "name": "Medium",
                    "size": "clamp(1rem, calc(0.13vw + 0.97rem), 1.13rem)",
                    "slug": "medium"
                },
                {
                    "name": "Small",
                    "size": "clamp(0.89rem, calc(-0.04vw + 0.9rem), 0.84rem)",
                    "slug": "small"
                },
                {
                    "name": "X Small",
                    "size": "clamp(0.79rem, calc(-0.16vw + 0.82rem), 0.63rem)",
                    "slug": "x-small"
                }
            ]
        },
        "blocks": {
            "core/heading": {
                "color": {
                    "background": false
                }
            }
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "blocks": {
            
        },
        "color": {
            "background": "var(--wp--preset--color--background)",
            "text": "var(--wp--preset--color--on-background)"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--custom--color--link)"
                }
            },
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--primary)",
                    "text": "var(--wp--preset--color--on-background)"
                },
                "border": {
                    "radius": "0px",
                    "width": "0px"
                },
                "spacing": {
                    "padding": {
                        "bottom": "0.75em",
                        "left": "1em",
                        "right": "1em",
                        "top": "0.75em"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "1"
                }
            },
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--mori)",
                    "fontWeight": "400",
                    "lineHeight": "var(--wp--custom--line-height--x-small)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--4-x-large)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3-x-large)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--2-x-large)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            }
        },
        "spacing": {
            "blockGap": "var(--wp--custom--layout--block-gap)",
            "padding": {
                "right": "calc(clamp(1.25rem, 3.75vw + 0.5rem, 5rem))",
                "left": "calc(clamp(1.25rem, 3.75vw + 0.5rem, 5rem))"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--inter)",
            "lineHeight": "var(--wp--custom--line-height--normal)",
            "fontSize": "var(--wp--preset--font-size--medium)"
        }
    }
}
