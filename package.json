{
    "name": "boilerpress",
    "version": "0.1.0",
    "description": "",
    "author": "Kenny Tran",
    "license": "MIT",
    "scripts": {
        "development": "webpack --config webpack/webpack.config.js --env NODE_ENV=development",
        "development:watch": "webpack --config webpack/webpack.config.js --env NODE_ENV=development --watch",
        "eslint": "eslint src/scripts/**/*.js",
        "eslint:fix": "eslint src/scripts/**/*.js --fix",
        "stylelint": "stylelint src/styles/**/*.scss",
        "stylelint:fix": "stylelint src/styles/**/*.scss --fix",
        "prettier": "prettier src/styles/**/*.scss src/scripts/**/*.js webpack/**/*.js",
        "prettier:fix": "prettier --write src/styles/**/*.scss src/scripts/**/*.js webpack/**/*.js",
        "blocks:start": "wp-scripts start --webpack-copy-php --webpack-src-dir=src/blocks --output-path=build/blocks",
        "blocks:build": "wp-scripts build --webpack-copy-php --webpack-src-dir=src/blocks --output-path=build/blocks",
        "theme:start": "yarn run development:watch",
        "theme:build": "webpack --config webpack/webpack.config.js --env NODE_ENV=production --env production",
        "start": "yarn run theme:start && yarn run blocks:start",
        "build": "yarn run theme:build && yarn run blocks:build"
    },
    "devDependencies": {
        "@babel/core": "^7.23.7",
        "@babel/eslint-parser": "^7.23.3",
        "@babel/preset-env": "^7.23.8",
        "@wordpress/scripts": "^27.1.0",
        "ajv": "^8.12.0",
        "autoprefixer": "^10.4.17",
        "babel-loader": "^9.1.3",
        "browser-sync": "^3.0.2",
        "browser-sync-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.9.1",
        "css-minimizer-webpack-plugin": "^6.0.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-webpack-plugin": "^4.0.1",
        "image-minimizer-webpack-plugin": "^4.0.0",
        "imagemin": "^8.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^10.0.1",
        "mini-css-extract-plugin": "^2.7.7",
        "node-sass-magic-importer": "^5.3.3",
        "postcss": "^8.4.33",
        "postcss-loader": "^8.0.0",
        "postcss-scss": "^4.0.9",
        "prettier": "^3.2.4",
        "sass": "^1.70.0",
        "sass-loader": "^14.0.0",
        "stylelint": "^16.2.0",
        "stylelint-scss": "^6.1.0",
        "stylelint-webpack-plugin": "^4.1.1",
        "terser-webpack-plugin": "^5.3.10",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-glob-entries": "^1.0.1",
        "webpack-glob-entry": "^2.1.1",
        "webpackbar": "^6.0.0"
    },
    "dependencies": {
        "gsap": "npm:@gsap/shockingly@^3.12.5"
    }
}
